{
  "name": "yicon",
  "version": "1.0.0",
  "description": "Font generation platform",
  "main": "bin/server.js",
  "scripts": {
    "lint": "eslint --ext .js,.jsx src",
    "start:dev": "better-npm-run start:dev",
    "start:prd": "better-npm-run start:prd",
    "client": "better-npm-run client",
    "dev": "concurrently --kill-others \"npm run client\" \"npm run start:dev\"",
    "prd": "concurrently --kill-others \"npm run build\" \"npm run start:prd\"",
    "db:sync": "better-npm-run database:sync",
    "db:force": "better-npm-run database:force",
    "db-dev:force": "better-npm-run database-dev:force",
    "test": "karma start",
    "build": "better-npm-run build",
    "sync": "better-npm-run build && node scripts"
  },
  "betterScripts": {
    "client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "BABEL_ENV": "development",
        "PORT": 3000
      }
    },
    "start:dev": {
      "command": "nodemon ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "BABEL_ENV": "development",
        "PORT": 3000
      }
    },
    "start:prd": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 3000
      }
    },
    "database:sync": {
      "command": "node ./bin/server.js",
      "env": {
        "SEQUELIZE_SYNC": "SYNC",
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000
      }
    },
    "database:force": {
      "command": "node ./bin/server.js",
      "env": {
        "SEQUELIZE_SYNC": "SYNC_FORCE",
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000
      }
    },
    "database-dev:force": {
      "command": "node ./bin/server.js",
      "env": {
        "SEQUELIZE_SYNC": "SYNC_FORCE",
        "NODE_PATH": "./src",
        "NODE_ENV": "dev",
        "PORT": 3000
      }
    },
    "build": {
      "command": "webpack --config webpack/prd.config.js && git add -f webpack-assets.json && git add -f static/dist/ && git commit -m \"rebuild for deploy\" && git push",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YMFE/yicon.git"
  },
  "keywords": [
    "react",
    "iconfont",
    "universal",
    "webpack",
    "express",
    "redux"
  ],
  "author": "malcolmyu <minghao_yu@163.com>",
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/YMFE/yicon/issues"
  },
  "engines": {
    "node": "~5.x",
    "npm": "~3.x"
  },
  "homepage": "https://github.com/YMFE/yicon#readme",
  "dependencies": {
    "axios": "^0.11.1",
    "classnames": "^2.2.5",
    "co": "^4.6.0",
    "cookie-parser": "~1.3.5",
    "core-decorators": "^0.12.3",
    "css-loader": "^0.23.1",
    "dateformat": "^1.0.12",
    "graphite": "0.0.6",
    "history": "^2.1.1",
    "iconfont-builder": "^2.3.0",
    "invariant": "^2.2.1",
    "isom-fetch": "^1.1.1",
    "koa": "^1.2.0",
    "koa-bodyparser": "^2.2.0",
    "koa-compress": "^1.0.9",
    "koa-favicon": "^1.2.1",
    "koa-logger": "^1.3.0",
    "koa-mount": "^1.3.0",
    "koa-router": "^5.4.0",
    "koa-session": "^3.3.1",
    "koa-static": "^2.0.0",
    "log4js": "^0.6.38",
    "multer": "^1.1.0",
    "mysql": "^2.10.2",
    "pinyin": "^2.8.0",
    "process": "^0.11.3",
    "q": "^1.4.1",
    "rc-dropdown": "^1.4.9",
    "rc-menu": "^4.13.0",
    "rc-notification": "^1.3.5",
    "rc-select": "^6.4.7",
    "react": "^15.3.0",
    "react-addons-css-transition-group": "^15.3.0",
